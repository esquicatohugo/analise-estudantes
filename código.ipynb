# Segundo gráfico: Gráfico Cartesiano - Vantagens Socioeconômicas
print("\nGRÁFICO 2: VANTAGENS SOCIOECONÔMICAS VS NOTAS MÉDIAS")
plt.figure(figsize=(12, 8))

vantagens = [
    "Baixa Escolaridade Parental\nAlimentação Gratuita\nSem Curso Preparatório",
    "Baixa Escolaridade Parental\nAlimentação Gratuita\nCom Curso Preparatório",
    "Baixa Escolaridade Parental\nAlimentação Padrão\nSem Curso Preparatório",
    "Baixa Escolaridade Parental\nAlimentação Padrão\nCom Curso Preparatório",
    "Alta Escolaridade Parental\nAlimentação Gratuita\nSem Curso Preparatório",
    "Alta Escolaridade Parental\nAlimentação Gratuita\nCom Curso Preparatório",
    "Alta Escolaridade Parental\nAlimentação Padrão\nSem Curso Preparatório",
    "Alta Escolaridade Parental\nAlimentação Padrão\nCom Curso Preparatório"
]

# Calcular médias para cada combinação
medias_vantagens = []
for vantagem in vantagens:
    partes = vantagem.split('\n')
    escolaridade = "alta" if "Alta" in partes[0] else "baixa"
    alimentacao = "standard" if "Padrão" in partes[1] else "free/reduced"
    curso = "completed" if "Com" in partes[2] else "none"

    mask = (
        (df['parental level of education'].isin(alta_escolaridade if escolaridade == "alta" else baixa_escolaridade)) &
        (df['lunch'] == alimentacao) &
        (df['test preparation course'] == curso)
    )
    media = df[mask]['average_score'].mean()
    medias_vantagens.append(media)

# Calcular regressão linear
x = np.arange(len(vantagens))
y = np.array(medias_vantagens)

# Regressão linear
slope, intercept = np.polyfit(x, y, 1)
regression_line = slope * x + intercept

# Plotar gráfico cartesiano com pontos originais
plt.plot(range(len(vantagens)), medias_vantagens, marker='o', linewidth=3, markersize=10, color='#2E8B57', markerfacecolor='white', markeredgewidth=2, label='Dados Observados')

# Plotar linha de regressão linear
plt.plot(x, regression_line, 'r--', linewidth=2, label=f'Regressão Linear (y = {slope:.2f}x + {intercept:.2f})')

plt.xlabel('Vantagens Socioeconômicas', fontweight='bold', fontsize=12)
plt.ylabel('Média de Notas', fontweight='bold', fontsize=12)
plt.title('VANTAGENS SOCIOECONÔMICAS VS DESEMPENHO ACADÊMICO', fontweight='bold', fontsize=14, pad=20)
plt.xticks(range(len(vantagens)))
plt.gca().set_xticklabels(vantagens, rotation=45, ha='right', fontsize=9)
plt.grid(True, alpha=0.3)
plt.legend()

for i, (v, m) in enumerate(zip(vantagens, medias_vantagens)):
    plt.annotate(f'Média {m:.1f}', (i, m), textcoords="offset points", xytext=(0,15),
                ha='center', fontweight='bold', fontsize=9, bbox=dict(boxstyle="round,pad=0.3", facecolor='yellow', alpha=0.7))

plt.tight_layout()
plt.show()

